{
  "name": "office-addin-challenge",
  "version": "0.0.1",
  "config": {
    "app_to_debug": "word",
    "app_type_to_debug": "desktop",
    "dev_server_port": 3000
  },
  "scripts": {
  "dev": "vite",
  "build": "vite build",
  "certs": "office-addin-dev-certs install",
  "start": "concurrently \"npm run dev\" \"office-addin-debugging start manifest.xml\"",
  "stop": "office-addin-debugging stop manifest.xml",
  "validate": "office-addin-manifest validate manifest.xml"
},
  "devDependencies": {
    "@types/office-js": "^1.0.377",
    "concurrently": "^8.2.2",
    "http-server": "^14.1.1",
    "office-addin-debugging": "^5.1.6",
    "office-addin-dev-certs": "^1.13.5",
    "office-addin-manifest": "^1.13.6",
    "typescript": "^5.4.2",
    "vite": "^7.3.0"
  },
  "dependencies": {
    "fs": "^0.0.1-security"
  }
}
